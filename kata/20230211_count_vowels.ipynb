   "source": [
    "def count_vowel(sentence):\n",
    "    count=0\n",
    "    for i in sentence:\n",
    "        if i in \"aeiou\":\n",
    "            count=count+1\n",
    "    return count\n",
    "        \n",
    "print(count_vowel(\"aeiou\"))\n",
    "        "
   ]
