   "source": [
    "SELECT \n",
    "  RANK() OVER (ORDER BY SUM(points) DESC) AS rank, \n",
    "  COALESCE(NULLIF(clan, ''), '[no clan specified]') AS clan, \n",
    "  SUM(points) AS total_points, \n",
    "  COUNT(*) AS total_people \n",
    "FROM people \n",
    "GROUP BY clan \n",
    "ORDER BY total_points DESC;"
   ]
  }
